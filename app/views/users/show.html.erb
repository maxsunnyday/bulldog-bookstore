<h1><%= @user.first_name %> <%= @user.last_name %></h1>
<h3><%= @user.email %></h3>
<h2>Your Listings</h2>
<ul>
    <% @user.listings.each do |listing| %>
        <li><%= link_to "#{listing.name}", listing_path(listing) %></li>
    <% end %>
</ul>

<% if @user.orders != [] %>
    <h2>Your Orders</h2>
    <ul>
        <% @user.order_listings.each do |listing| %>
            <li><%= link_to "#{listing.name}", listing_path(listing) %></li>
        <% end %>
    </ul>
<% end %>